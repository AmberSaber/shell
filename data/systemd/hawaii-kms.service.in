[Unit]
Description=Hawaii (KMS)

[Service]
Type=notify
Environment="DBUS_SESSION_BUS_ADDRESS=unix:path=%t/bus" QT_QPA_EGLFS_INTEGRATION=eglfs_kms SAL_USE_VCLPLUGIN=kde QT_QPA_PLATFORMTHEME=Hawaii XDG_MENU_PREFIX=hawaii- XDG_CURRENT_DESKTOP=Hawaii
ExecStart=@CMAKE_INSTALL_FULL_BINDIR@/hawaii-session --mode="eglfs"

[Install]
Alias=hawaii.service
WantedBy=hawaii.target
